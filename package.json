{
  "name": "saas-dashboard",
  "version": "1.0.0",
  "description": "Modern SaaS Dashboard with React",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
